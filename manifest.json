{
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "short_name": "OCJJ",
  "description": "__MSG_appDesc__",
  "default_locale": "zh_CN",
  "version": "0.1.0",
  "browser_action": {
    "default_icon": "green.png"
  },
  "content_scripts": [
    {
      "matches": ["http://www.bilibili.com/", "http://www.bilibili.com/*"],
      "js": ["jquery-3.1.1.min.js", "lodash.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "optional_permissions": [
    "tabs"
  ]
}
